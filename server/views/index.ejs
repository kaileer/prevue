<script src="https://cdn.jsdelivr.net/npm/dplayer/dist/DPlayer.min.js"></script>
<script>
    var dp = null;
    $(document).ready(function(){
        $('.file-box').each(function() {
            animationHover(this, 'pulse');
        });
    });
    $('.mask').click(function () {
        $('.page-dialog').hide();
        dp.pause()
    })
    $('.j-file').click(function () {
        var op = {
            url: $(this).attr('data-video'),
            pic: $(this).attr('data-src'),
        }
        $('.page-dialog').show();
        if(!dp){
           dp = new DPlayer({
                container: document.getElementById('dplayer'),
                screenshot: true,
                video: {
                    url: op.url,
                    pic: op.pic,
                    thumbnails: 'thumbnails.jpg'
                }
            }); 
        }else{
            if (dp.video.currentSrc !== op.url) {
                dp.switchVideo({
                    url: op.url,
                    pic: op.pic,
                    type: 'auto'
                })
            }
        }
    })
    $('.j-edit').click(function () {
        var that = $(this)
        $.ajax({
            url: '/student/list',
            type: 'post',
            dataType: 'json',
            data:{
                sid: that.attr('sid')
            },
            success: function (data) {
                console.log(data)
                location.reload();
            }
        })
    })
    
    function showDialog(text) {
        $('.mask').show();
        
    }
</script>

